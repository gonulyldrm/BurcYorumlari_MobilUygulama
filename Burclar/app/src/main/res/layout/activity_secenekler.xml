<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/multi"
    android:layout_height="match_parent"
    tools:context=".Secenekler">


    <Button
        android:id="@+id/yengecc"
        android:layout_width="142dp"
        android:layout_height="46dp"
        android:layout_marginStart="47dp"
        android:layout_marginBottom="13dp"
        android:text="yengec"
        app:layout_constraintBottom_toTopOf="@+id/basakk"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/basakk"
        android:layout_width="142dp"
        android:layout_height="46dp"
        android:layout_marginStart="47dp"
        android:layout_marginBottom="23dp"
        android:text="basak"
        app:layout_constraintBottom_toTopOf="@+id/terazii"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/yayy"
        android:layout_width="142dp"
        android:layout_height="46dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="13dp"
        android:text="yay"
        app:layout_constraintBottom_toTopOf="@+id/oglakk"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/terazii"
        android:layout_width="142dp"
        android:layout_height="46dp"
        android:layout_marginStart="47dp"
        android:layout_marginBottom="84dp"
        android:text="terazi"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/kovaa"
        android:layout_width="142dp"
        android:layout_height="46dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="84dp"
        android:text="kova"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/oglakk"
        android:layout_width="142dp"
        android:layout_height="46dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="23dp"
        android:text="oglak"
        app:layout_constraintBottom_toTopOf="@+id/kovaa"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/akrepp"
        android:layout_width="142dp"
        android:layout_height="46dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="23dp"
        android:text="akrep"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/balikk" />

    <Button
        android:id="@+id/kocc"
        android:layout_width="142dp"
        android:layout_height="46dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="60dp"
        android:text="koc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bogaa" />

    <Button
        android:id="@+id/balikk"
        android:layout_width="142dp"
        android:layout_height="46dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="13dp"
        android:text="balik"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/aslann" />

    <Button
        android:id="@+id/bogaa"
        android:layout_width="142dp"
        android:layout_height="46dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="60dp"
        android:text="boga"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ikizlerr" />

    <Button
        android:id="@+id/ikizlerr"
        android:layout_width="142dp"
        android:layout_height="46dp"
        android:layout_marginTop="114dp"
        android:layout_marginEnd="60dp"
        android:text="ikizler"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/aslann"
        android:layout_width="142dp"
        android:layout_height="46dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="114dp"
        android:text="aslan"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>